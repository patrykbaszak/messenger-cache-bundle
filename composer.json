{
    "name": "pbaszak/messenger-cache-bundle",
    "type": "symfony-bundle",
    "require": {
        "php": ">=8.1",
        "symfony/messenger": ">=5.4",
        "symfony/cache": ">=5.4",
        "symfony/config": ">=5.4",
        "symfony/dependency-injection": ">=5.4",
        "symfony/http-kernel": "^6.2"
    },
    "require-dev": {
        "symfony/phpunit-bridge": ">=5.4",
        "phpstan/phpstan": "^1.9",
        "phpstan/phpstan-symfony": "^1.2",
        "phpstan/phpdoc-parser": "^1.15",
        "phpstan/phpstan-phpunit": "^1.3",
        "phpstan/extension-installer": "^1.2",
        "friendsofphp/php-cs-fixer": "^3.13",
        "phpunit/phpunit": "^9.5",
        "symfony/framework-bundle": "^6.2",
        "symfony/yaml": "^6.2"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "PBaszak\\MessengerCacheBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PBaszak\\MessengerCacheBundle\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "patrykbaszak",
            "email": "patryk.baszak@gmail.com"
        }
    ],
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "scripts": {
        "cache:clear": "rm -rf var/cache/*",
        "code:fix": "vendor/bin/php-cs-fixer fix",
        "code:analyse": "vendor/bin/phpstan analyse src -c tools/phpstan/fpm-config.neon",
        "code:analyse:generate-baseline": "vendor/bin/phpstan analyse src -c tools/phpstan/fpm-config.neon --generate-baseline tools/phpstan/fpm-baseline.neon --allow-empty-baseline",
        "phpunit": "vendor/bin/phpunit",
        "test:unit": "@phpunit --group unit tests",
        "test:func": "@phpunit --group func tests"
    }
}
